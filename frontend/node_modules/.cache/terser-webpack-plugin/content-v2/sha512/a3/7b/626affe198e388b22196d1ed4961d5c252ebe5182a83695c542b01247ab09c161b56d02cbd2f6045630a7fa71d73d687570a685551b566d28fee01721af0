    this.setState({\r\n                       shortURL: res.data.short_url,\r\n                       statusCode: 1\r\n                     })\r\n                   } else {\r\n                     this.setState({\r\n                       statusCode: -1\r\n                     })\r\n                   }\r\n                 })\r\n                 .catch(err => console.log(err));\r\n  };\r\n  render() {\r\n    return (\r\n      <div className={this.props.className}>\r\n        <h2>URL Shortener</h2>\r\n        <p>Here you can enter some long URL-address to get short one.</p>\r\n        {this.state.shortURL && ( this.state.statusCode > 0 ? (\r\n          <Alert variant=\"info\">\r\n            Wow! Keep an eye on this url:\r\n            <Alert.Link href={this.state.shortURL}>\r\n              {this.state.shortURL}\r\n            </Alert.Link>\r\n          </Alert>\r\n        ) : (\r\n          <Alert variant=\"warning\">\r\n            Sorry! Seems like this address is invalid...\r\n          </Alert>\r\n        )\r\n        )}\r\n        <InputGroup>\r\n          <FormControl\r\n            placeholder=\"http://www.example.com\"\r\n            value={this.state.originalURL}\r\n            onChange={this.handleChange.bind(this)}\r\n          />\r\n          <InputGroup.Append>\r\n            <Button variant=\"primary\" onClick={this.handleSubmit.bind(this)}>Get short</Button>\r\n          </InputGroup.Append>\r\n        </InputGroup>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n","import axios from 'axios';\r\n\r\nexport const getShortener = (url) => axios.post('/api/shorturl', {url});\r\nexport const getOriginal = (id) => axios.post('/api/shorturl/' + id)\r\n","import './App.css';\nimport { BrowserRouter, Route, Switch } from 'react-router-dom';\nconst { URLShortener } = require('./components/URLShortener');\n\nfunction App() {\n  return (\n    <BrowserRouter>\n      <div id=\"App\">\n        <h1>Front-End to interact with Microservices</h1>\n        <Switch>\n          